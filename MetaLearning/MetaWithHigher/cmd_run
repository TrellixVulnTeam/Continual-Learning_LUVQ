#Omniglot Dataset

nohup python MamlWithHigher.py --wand-project MetaLearning_Omniglot --wand-note Higher_Omniglot_5W_5S_AStep_5_MetaLearn_False --dataset omniglot --ways 5 --shots 5 --adaptation-steps 5 --input-dim 28 --model-name vanilla --wandb-logging 1 --num-epochs 200000 --meta-learn 0 &



